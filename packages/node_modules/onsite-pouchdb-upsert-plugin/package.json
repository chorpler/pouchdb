{
  "name": "@onsite/pouchdb-upsert-plugin",
  "version": "3.0.0-rc.4",
  "description": "PouchDB plugin with upsert() and putIfNotExists() functions, forked for PouchDB 7.0.0+",
  "homepage": "https://github.com/chorpler/pouchdb-upsert-plugin#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/chorpler/pouchdb-upsert-plugin.git"
  },
  "keywords": [
    "pouch",
    "pouchdb",
    "upsert",
    "putIfNotExists",
    "couch",
    "couchdb"
  ],
  "author": "David Sargeant <chorpler@gmail.com>",
  "contributors": [
    "Nolan Lawson <nolan@nolanlawson.com> (https://nolanlawson.com/)"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/chorpler/pouchdb-upsert-plugin/issues"
  },
  "files": [
    "compiled",
    "dist",
    "docs",
    "lib",
    "src",
    "types"
  ],
  "main": "lib/index.js",
  "jsnext:main": "lib/index.es.js",
  "module": "lib/index.es.js",
  "browser": "lib/index.browser.js",
  "types": "types/index.d.ts",
  "scripts": {
    "clean": "rimraf lib/* dist/* compiled/* && mkdirp lib dist compiled",
    "compile": "tsc -p ./tsconfig.json",
    "rollup-cjs": "rollup -c config/rollup.config.cjs.js && rollup -c config/rollup.config.browser.cjs.js",
    "rollup-es": "rollup -c config/rollup.config.es.js && rollup -c config/rollup.config.browser.es.js",
    "rollup": "npm-run-all --parallel rollup-cjs rollup-es",
    "browserify:rollup": "browserify -t [ babelify --global --presets [ @babel/preset-env ] ] -t brfs -p bundle-collapser/plugin -s PouchUpsertPlugin lib/index.browser.js > dist/pouchdb.upsert.plugin.js",
    "browserify": "npm run browserify:rollup",
    "minify": "uglifyjs -mc < dist/pouchdb.upsert.plugin.js > dist/pouchdb.upsert.plugin.min.js",
    "build:prod": "npm-run-all clean rollup browserify minify",
    "build:dev": "npm-run-all clean rollup browserify",
    "build": "npm run build:prod",
    "prepublishOnly": "npm run build",
    "lint": "eslint bin/ src/ test/",
    "dev": "npm run test-local",
    "test": "npm run rollup-cjs && node bin/run-test.js",
    "test-local": "cross-env CLIENT=local npm run test",
    "test-node": "cross-env CLIENT=node TEST_DB=testdb,http://localhost:5984/testdb npm run test",
    "test-browser": "cross-env CLIENT=ChromeHeadless npm run test",
    "test-types": "tsc --noEmit -p types",
    "release": "standard-version"
  },
  "scriptsComments": {
    "build-old": "tsc && mkdir -p dist && browserify index.js -t es3ify -s PouchUpsert -o dist/pouchdb.upsert.js && npm run min",
    "oldScripts": {
      "test-phantom": "cross-env CLIENT=phantom npm run test",
      "browserify": "browserify -t brfs -p bundle-collapser/plugin -s PouchUpsertPlugin lib/index.browser.js > dist/pouchdb.upsert.plugin.js",
      "test-node": "cross-env TEST_DB=testdb,http://localhost:5984/testdb istanbul test ./node_modules/mocha/bin/_mocha test/test.js",
      "test-local": "zuul --ui mocha-bdd --no-coverage --local 9000 test/test.js",
      "test": "npm run jshint && ./bin/run-test.sh",
      "test-browser": "zuul --no-coverage --phantom --ui mocha-bdd test/test.js",
      "build": "npm run compile && browserify index.js -t es3ify -s PouchUpsert -o dist/pouchdb.upsert.js && npm run min",
      "min": "uglifyjs dist/pouchdb.upsert.js -mc > dist/pouchdb.upsert.min.js",
      "coverage": "npm test --coverage && istanbul check-coverage --lines 100 --function 100 --statements 100 --branches 100"
    }
  },
  "dependencies": {
    "inherits": "2.0.3",
    "pouchdb-binary-utils": "^7.2.2",
    "pouchdb-core": "^7.2.2",
    "pouchdb-fetch": "^7.2.2",
    "pouchdb-utils": "^7.2.2"
  },
  "dependenciesComments": {
    "REMOVED-pouchdb-promise": "^6.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@onsite/pouchdb-memory": "^7.0.0-rc.1",
    "@types/debug": "^4.1.1",
    "@types/node-fetch": "^2.1.6",
    "@types/pouchdb": "^6.3.3",
    "@types/pouchdb-core": "^7.0.2",
    "@types/pouchdb-find": "^6.3.4",
    "@types/pouchdb-replication": "^6.4.2",
    "add-cors-to-couchdb": "0.0.6",
    "babelify": "^10.0.0",
    "brfs": "^2.0.1",
    "browserify": "^16.2.3",
    "bundle-collapser": "^1.3.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.2.0",
    "es3ify": "^0.2.2",
    "es6-promise": "^4.2.6",
    "eslint": "^5.14.1",
    "istanbul": "^0.4.5",
    "jshint": "^2.10.1",
    "json8": "^0.9.2",
    "karma": "^4.0.0",
    "karma-browserify": "^6.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sauce-launcher": "^2.0.2",
    "mkdirp": "^0.5.1",
    "mocha": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "pouchdb-adapter-http": "^7.2.2",
    "pouchdb-server": "^4.2.0",
    "promise-polyfill": "^8.2.0",
    "puppeteer": "^1.12.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.2.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-inject": "^2.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-typescript2": "^0.19.2",
    "standard-version": "^5.0.0",
    "tslib": "^1.9.3",
    "typescript": "^3.2.2",
    "uglify-js": "^3.4.9",
    "watchify": "^4.0.0",
    "whatwg-fetch": "^3.0.0",
    "zuul": "^3.12.0"
  },
  "devDependenciesComments": {
    "removed": {
      "pouchdb-memory-git": "git+https://github.com/chorpler/pouchdb-memory",
      "bluebird": "^3.4.7",
      "pouchdb-memory": "file:../pouchdb-memory"
    }
  },
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  }
}
